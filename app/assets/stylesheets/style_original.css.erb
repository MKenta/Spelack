textarea {
    font-size: 1rem;
    line-height: 1.25rem;
    width: 100%;
    padding: .75rem .5rem .75rem .75rem;
    vertical-align: top;
}

hr {
  border-top: 1px solid rgb(153, 153, 153);
}

hr.or {
  margin-top: 20px;
  margin-bottom: 20px;
  border-top: 1px solid #f8f5f0;
  text-align: center;
}

hr.or:after {
  content: 'or';
  display: inline-block;
  position: relative;
  top: -12px;
  padding: 0 10px;
  /*background: #fff;*/
  background-color: rgba( 255, 255, 255, 0);
  color: #000;
  font-size: 15px;
}

.fb:before
{
  font-family: 'FontAwesome' ;
  padding-right: 5px ;
}

.btn.focus, .btn:focus, .btn:hover {
    color: #fff;
    text-decoration: none;
}

label.required:before{
  content: '*';
  padding-right: 5px;
  color: red;
}

.channel_browser_row:hover {
  background: #edf7fd;
  border: 1px solid #d3ecfa;
  border-radius: 6px;
  cursor: pointer;
}

.channel_browser_row {
  display: block;
  width: 100%;
  min-height: 60px;
  padding: 10px;
  border: 1px solid transparent;
  border-top: 1px solid #e8e8e8;
  cursor: pointer;
  font-size: .875rem;
  line-height: 1.4;
  color: #9e9ea6;
}

.channel_browser_row_header {
  width: 100%;
  font-size: 1rem;
  margin-bottom: 3px;
  color: #555459;
  font-size: 16px;
}

.channel_browser_divider {
  color: #67686e;
  font-size: 15px;
  height: 35px;
  line-height: 2.1875rem;
  width: 100%;
  padding-left: 1rem;
  font-weight: 700;
  background: #fff;
}

.overflow_ellipsis {
  display: block;
  text-overflow: ellipsis;
