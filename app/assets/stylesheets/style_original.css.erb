footer {
  position: fixed;
  background: #fff;
  bottom: 0;
  left: 250px;
  right: 1rem;
  height: 4rem;
  /*z-index: 52;*/
}
#footer_overlay {
  background: #fff;
  opacity: 1;
  -moz-opacity: 1;
  -khtml-opacity: 1;
  -webkit-transition: opacity ease-in .35s 0s;
  -moz-transition: opacity ease-in .35s 0s;
  transition: opacity ease-in .35s 0s;
  height: 4rem;
}

.hidden {
